@using Action = Papagames.Detective.Core.Game.Action
@model Papagames.Detective.App.Web.Models.HistoryModel
@{
    var emotionType = (Action) ViewData["EmotionType"];
}
<ul>
    @foreach (var record in Model.EmotionSubjects(emotionType))
    {
        // Book: Process.EmotionList View 

        <li>
            @record.Subject.Name:
            <ul>
                @foreach (var emotion in Model.EmotionValues(emotionType, record.Subject.Number))
                {
                    <li>@emotion.Agent: @emotion.Value</li>
                }
            </ul>
        </li>
    }
</ul>