@model Crimenuts.App.Web.Models.SchemaModel
@{
    ViewBag.Title = "Detective Process";
    Layout = "~/Views/Shared/Layouts/Default.cshtml";
}

<h2>Processes</h2>
<ul>
    @foreach (var p in Model.Processes)
    {
        <li>
            @Html.ActionLink(p.ShortInfo, "Info", new { id = p.Id }) @Html.ActionLink("(New Play)", "ClassicPlay", new { id = p.Id })
        </li>
    }
    @if (Model.Processes.Count == 0)
    {
        @:No processes found
    }
</ul>